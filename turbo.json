{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {},
    "test": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": ["^build"],
      "env": ["OPENAI_API_KEY"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "start:dev": {
      "dependsOn": ["api#infra"],
      "cache": false
    },
    "infra": {
      "cache": false
    }
  }
}
